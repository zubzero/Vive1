<?xml version='1.0' encoding='utf-8'?>
<widget id="com.viveenunclick.mx" version="0.36" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vive En Un Click</name>
    <description>
        Red de negocios
    </description>
    <author email="bernagaufo@hotmail.com" href="https://facebook.com/villbercreativo">
        Villber Guadalajara
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="/img/splash.jpg" />
    <preference name="Orientation" value="portrait" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="SplashScreen" value="screen" />
</widget>
